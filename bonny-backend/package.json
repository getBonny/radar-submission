{
  "name": "bonny-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/config/data-source.ts",
    "migration:generate": "npm run typeorm:cli migration:generate",
    "migration:create": "npm run typeorm:cli migration:create",
    "migration:run": "npm run typeorm:cli migration:run",
    "migration:revert": "npm run typeorm:cli migration:revert"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@google-cloud/cloud-sql-connector": "^1.2.2",
    "@google-cloud/pubsub": "^4.3.3",
    "@langchain/core": "^0.2.31",
    "@langchain/openai": "^0.2.8",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex/js": "^4.12.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@reclaimprotocol/js-sdk": "^1.3.11",
    "@solana/spl-token": "^0.4.1",
    "@solana/web3.js": "^1.95.2",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.6.8",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "class-transformer": "^0.5.1",
    "crypto": "^1.0.1",
    "discord.js": "^14.15.3",
    "firebase-admin": "^12.0.0",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-firebase-jwt": "^1.2.1",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "telegraf": "^4.16.3",
    "tweetnacl": "^1.0.3",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.1.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/*.d.ts",
      "!main.ts",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./reports/junit",
          "outputName": "js-test-results.xml"
        }
      ]
    ]
  }
}
